[EDTTEC] Processus SQRibe
5 Selection de l edition
  A export EDITION_NAME=`echo #edition_name `
5 Run time SQR
  D sqr $EDITION_NAME.sqr ${AUC_DBUSER}/${AUC_DBPASSWD}@${ORACLE_SID}
5 Compilation SQRmake
  D sqrmake -f $EDITION_NAME.sqr -o ${AUC_DBUSER} -p ${AUC_DBPASSWD} -b ${ORACLE_SID}
5 Lancement edition SQRT
  A export EDT_LAN="2900020"
  A export EDT_DEV="3000024"
  A export EDT_USR="$AUC_DBUSER"
  A export EDT_SITE="1"
  D sqrt $EDITION_NAME.sqt ${AUC_DBUSER}/${AUC_DBPASSWD}@${ORACLE_SID} -e -o "$EDT_LAN" "$EDT_DEV" "$EDT_USR" "$EDT_SITE" #param_applicatif -KEEP -PRINTER:HP 
  A export LPDEST=OptraTest1115F
  D lp $EDITION_NAME.lis
1 Return to Main menu
  M MAIN

[EDTRUN] Edition
3 Modification des parametres d'env.
  P $HOME/${CCAT_NIV_TST}/${REPORT}_${CCAT_COD_TST}/technical c1_par.lst 1
3 Chargement du Jeux de Test
  A tpdload $DT128B 1
3 Lancement de l edition
  D $CCAT_BatchPublish/bin/jaa010e0 OFFLINE "$CCAT_BATCH_LAN" "$COD_DEVISE" $REPORT "" $LPDEST 1 1 1 1 -param "$CCAT_BATCH_LAN" "$COD_DEVISE" "$CCAT_BATCH_DB" "$COD_SITE" $OTHER_PARAM
4 Extraction du resultats
  A tpdxtr  $DT128B 1
4 Analyse du resultats
  A bchcomp $DT128B 1
4 Comparaison des fichiers en sortie
  B bchfilecmp $DT128B 1 $HOME/ablytests/${DT128B}_at/technical/c1_efl.lst
1 Return to previous menu
  A export cpl_title=
  M EDT111

